{
	"name": "Backend",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "env-cmd node index.js",
		"dev": "env-cmd nodemon rs",
		"lint": "xo"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"body-parser": "^1.19.0",
		"express": "^4.17.1",
		"jsonfile": "^6.0.1",
		"qs": "^6.9.4",
		"swagger-jsdoc": "^4.2.3",
		"swagger-ui-dist": "^3.35.1",
		"swagger-ui-express": "^4.1.4",
		"uuid": "^8.2.0"
	},
	"devDependencies": {
		"env-cmd": "10.1.0",
		"nodemon": "2.0.4",
		"xo": "0.30.0"
	},
	"xo": {
		"rules": {
			"quote-props": "warn",
			"eqeqeq": "off",
			"array-callback-return": "warn",
			"require-atomic-updates": "warn",
			"no-else-return": "warn",
			"camelcase": "off",
			"no-unused-vars": "warn",
			"no-return-await": "warn"
		},
		"semicolon": false
	}
}
